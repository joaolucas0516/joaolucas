algoritmo "Área_do_retangulo"
// Autor : João Lucas
// Data : 14/09/2017
// Cálculo da área do retângulo
var
  comprimento,largura,area  : real
inicio
      escreva("Qual o comprimento? ")
      leia(comprimento)
      escreva("Qual a largura? ")
      leia(largura)
      area <- comprimento * largura
      escreva("A área é",area)
fimalgoritmo
